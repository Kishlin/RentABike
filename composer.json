{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.4",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.10",
        "ramsey/uuid": "^4.2",
        "symfony/console": "6.0.*",
        "symfony/dotenv": "6.0.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.0.*",
        "symfony/proxy-manager-bridge": "6.0.*",
        "symfony/runtime": "6.0.*",
        "symfony/service-contracts": "^2.5",
        "symfony/yaml": "6.0.*"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Kishlin\\Apps\\Backoffice\\Backend\\": "apps/backoffice/backend/src",
            "Kishlin\\Apps\\Rentabike\\Backend\\": "apps/rentabike/backend/src",

            "Kishlin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kishlin\\Tests\\Apps\\Backoffice\\Backend\\": "apps/backoffice/backend/tests",
            "Kishlin\\Tests\\Apps\\Rentabike\\Backend\\": "apps/rentabike/backend/tests",

            "Kishlin\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.0.*"
        }
    },
    "require-dev": {
        "behat/behat": "^3.10",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.0.*"
    }
}
